{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "orderItems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "order"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "PLACED",
        "IN_TRANSIT",
        "OUT_FOR_DELIVERY",
        "DELIVERED"
      ]
    },
    "orderLocationHistories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-location-history.order-location-history"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "deliveryAddress": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::address.address",
      "inversedBy": "ordersDelivery"
    },
    "billingAddress": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::address.address",
      "inversedBy": "ordersBilling"
    }
  }
}
